port_B0 motor OUTPUT!
port_B1 motor OUTPUT1!
port_B2 motor OUTPUT2!
port_A0 button INPUT1!
port_A1 button INPUT2!
port_C2 LED Led!

function IniciarMotor(){
   start(OUTPUT,INPUT1)!
   restart(OUTPUT,INPUT1)!
}
function MoverMotor(){
   move(OUTPUT,180,INPUT2)!
}

loop{
   call IniciarMotor()!
   ligther(Led,INPUT1)!
   call MoverMotor()!
   ligther(Led,INPUT2)!
}